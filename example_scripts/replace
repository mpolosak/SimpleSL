replace = (array: [any], i1: int, i2: int)->[any]{
    e1 = array[i1];
    e2 = array[i2];
    array1 = array ? (index: int, element: any) -> int {
        index < i1
    }
    array2 = array ? (index: int, element: any) -> int {
        index > i1 && index < i2
    }
    array3 = array ? (index: int, element: any) -> int {
        index > i2
    }
    array1 + [e2] + array2 + [e1] + array3
}