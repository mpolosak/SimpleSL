fizzbuzz = (number: int) -> string|int {
    if number%15==0 {
        "FizzBuzz";
    } else if number%3==0 {
        "Fizz";
    } else if number%5==0 {
        "Buzz";
    } else {
        number;
    };
};

/*
Returns array filled whith increasing values, starting with value
Length of returned array equals size
*/
iota = (value: int, size: int) -> [int] {
    [value; size] @ (index: int, value:int) -> int {index+value;};
};

array = iota(1, 15);
array = array @ fizzbuzz;
array @ print;