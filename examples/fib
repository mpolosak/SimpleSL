int = (variable: any, fallback: int) -> int {
    match variable {
        value: int => value,
        => fallback,
    };
};

fib = (n:int) -> [int] {
    array = recsub([0,1], (array:[any]) -> int {
        len = len(array);
        a = array[len-1];
        b = array[len-2];
        int(a, 0)+int(b, 0);
    }, 70);
    array ? int;
};